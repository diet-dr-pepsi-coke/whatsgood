<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/star1"
    android:layout_width="match_parent"
    android:contentDescription="onestar"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="387dp"
        android:layout_height="190dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="363dp"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/username"
                android:layout_width="191dp"
                android:layout_height="23dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="16dp"
                android:fontFamily="sans-serif"
                android:text="sample food"
                android:textColor="@color/purple_200"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/rstar1"
                android:layout_width="34dp"
                android:layout_height="27dp"
                android:layout_marginStart="7dp"
                android:layout_marginBottom="124dp"
                android:backgroundTint="#FAF7F7"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@android:drawable/star_off" />

            <ImageButton
                android:id="@+id/rstar2"
                android:layout_width="34dp"
                android:layout_height="27dp"
                android:layout_marginBottom="124dp"
                android:backgroundTint="#FAF7F7"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.011"
                app:layout_constraintStart_toEndOf="@+id/rstar1"
                app:srcCompat="@android:drawable/star_off" />

            <ImageButton
                android:id="@+id/rstar3"
                android:layout_width="34dp"
                android:layout_height="27dp"
                android:layout_marginBottom="124dp"
                android:backgroundTint="#FAF7F7"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/rstar2"
                app:srcCompat="@android:drawable/star_off" />

            <ImageButton
                android:id="@+id/rstar4"
                android:layout_width="34dp"
                android:layout_height="27dp"
                android:layout_marginEnd="196dp"
                android:layout_marginBottom="124dp"
                android:backgroundTint="#FAF7F7"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@android:drawable/star_off" />

            <ImageButton
                android:id="@+id/rstar5"
                android:layout_width="34dp"
                android:layout_height="27dp"
                android:layout_marginBottom="124dp"
                android:backgroundTint="#FAF7F7"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/rstar4"
                app:srcCompat="@android:drawable/star_off" />

            <TextView
                android:id="@+id/reviewtxt"
                android:layout_width="322dp"
                android:layout_height="91dp"
                android:layout_marginStart="26dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="16dp"
                android:text="mock review . . ."
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>
